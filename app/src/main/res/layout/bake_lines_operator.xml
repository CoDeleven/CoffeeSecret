<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:paddingLeft="5dp"
              android:paddingTop="12dp">

    <CheckBox
        android:id="@+id/id_baking_line_bean"
        style="@style/bakingLinesCheckbox"
        android:text="豆温　　　"/>

    <CheckBox
        android:id="@+id/id_baking_line_inwind"
        style="@style/bakingLinesCheckbox"
        android:text="进风温　　"/>

    <CheckBox
        android:id="@+id/id_baking_line_outwind"
        style="@style/bakingLinesCheckbox"
        android:text="出风温　　"/>

    <CheckBox
        android:id="@+id/id_baking_line_accBean"
        style="@style/bakingLinesCheckbox"
        android:text="豆升温率　"/>

    <CheckBox
        android:id="@+id/id_baking_line_accInwind"
        style="@style/bakingLinesCheckbox"
        android:text="进风升温率"/>

    <CheckBox
        android:id="@+id/id_baking_line_accOutwind"
        style="@style/bakingLinesCheckbox"
        android:text="出风升温率"/>

</LinearLayout>
